#%RAML 1.0 DataType

type: object
displayName: OrderStatus
properties: 
  account: string
  startTime: datetime
  elapsedTimeMinutes: number
  assetPair: string
  status:
    enum: ["Failed", "Done", "In-Progress"]
  requestedSize: number
  executionSize:
    type: number
    description: Size executed so far.
  executedTargetAmount:
    type: number
    description: The amount of the target asset executed so far.
  executionMessage?: string
  tradeOrderId: string
  walletAddress: string

