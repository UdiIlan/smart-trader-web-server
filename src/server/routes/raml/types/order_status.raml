#%RAML 1.0 DataType

type: object
displayName: OrderStatus
properties: 
  account: string
  startTime: datetime
  elapsedTimeMinutes: number
  assetPair: string
  actionType: string
  status:
    enum: [Done, Failed, In-Progress]
  requestedSize: number
  requestedPrice: number
  executionSize:
    type: number
    description: Size executed so far.
  executedTargetAmount:
    type: number
    description: The amount of the target asset executed so far.
  executionMessage?: string
  tradeOrderId: string
  walletPlane: 
    type: array
    items:
      type: object
      properties:
        walletAddress: string
        amount: number
